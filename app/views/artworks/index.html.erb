<p id="notice"><%= notice %></p>

<h1>Artworks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Slug</th>
      <th>Category</th>
      <th>Medium</th>
      <th>Date</th>
      <th>Thumbnail</th>
      <th>Genre</th>
      <th>Url</th>
      <th>Artist</th>
      <th>Similar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artworks.each do |artwork| %>
      <tr>
        <td><%= artwork.title %></td>
        <td><%= artwork.slug %></td>
        <td><%= artwork.category %></td>
        <td><%= artwork.medium %></td>
        <td><%= artwork.date %></td>
        <td><%= artwork.thumbnail %></td>
        <td><%= artwork.genre %></td>
        <td><%= artwork.url %></td>
        <td><%= artwork.artist %></td>
        <td><%= artwork.similar %></td>
        <td><%= link_to 'Show', artwork %></td>
        <td><%= link_to 'Edit', edit_artwork_path(artwork) %></td>
        <td><%= link_to 'Destroy', artwork, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artwork', new_artwork_path %>
